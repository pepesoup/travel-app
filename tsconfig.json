{
    "compilerOptions": {
        "strict": true,
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "paths": {
            "@root/*": ["./*"],
            "@src/*": ["./src/*"],
            "@components/*": ["./src/components/*"],
            "@rn-components/*": ["./src/rn-components/src/components/*"]
        }
    },
    "extends": "expo/tsconfig.base",
    "typeRoots": ["./types"]
}
